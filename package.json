{
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:master": "cd master && umi dev",
    "start:app1": "cd app1 && umi dev",
    "start:app2": "cd app2 && umi dev",
    "start:app3": "cd app3 && umi dev"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.1.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.1.2",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "mockjs": "^1.0.1-beta3",
    "npm-run-all": "^4.1.5",
    "@umijs/plugin-qiankun": "^2.2.0-0"
  }
}
